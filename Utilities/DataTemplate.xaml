<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CurrencyWPF.ViewModels;assembly=CurrencyWPF"
                    xmlns:views="clr-namespace:CurrencyWPF.Views;assembly=CurrencyWPF">
    <DataTemplate DataType="{x:Type vm:AssetsPageVM}">
        <views:AssetsPage/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CurrencyPageVM}">
        <views:CurrencyPage />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainWindowVM}">
        <views:MainWindow/>
    </DataTemplate>
</ResourceDictionary>